import svunit_pkg::*;

module TS_TYPE;
  string name = "TS_INST";

  //===================
  // Test suite master
  //===================
  svunit_testsuite svunit_ts;

  //===================================
  // These are the unit tests that we
  // want included in this testsuite
  //===================================
  virtual_test_unit_test virtual_test_ut();


  //===================================
  // Setup
  //===================================
  function void setup();
    virtual_test_ut.setup();
    svunit_ts = new(name);
    svunit_ts.add_testcase(virtual_test_ut.unittest);
  endfunction

endmodule


