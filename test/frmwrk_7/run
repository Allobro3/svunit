# remove and create the unit_test

# invoke create_svunit.pl (nothing should happen and should return 1)
create_svunit.pl
if [ "$?" != "1" ]; then
  echo create_svunit.pl returned $?
  exit 1
fi

# verify no files were created
for f in `ls`; do
  if [ "$f" != "clean" -a "$f" != "run" ]; then
    echo bad file created: $f
    exit 1
  fi
done

exit 0
