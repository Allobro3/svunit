import svunit_pkg::*;

module PWD_testsuite;
  string name = "PWD_ts";

  //===================
  // Test suite master
  //===================
  svunit_testsuite svunit_ts;

  //===================================
  // These are the unit tests that we
  // want included in this testsuite
  //===================================
  test_if_unit_test test_if_ut();
  test_if2_unit_test test_if2_ut();


  //===================================
  // Setup
  //===================================
  function void setup();
    test_if_ut.setup();
    test_if2_ut.setup();
    svunit_ts = new(name);
    svunit_ts.add_testcase(test_if_ut.unittest);
    svunit_ts.add_testcase(test_if2_ut.unittest);
  endfunction

endmodule


